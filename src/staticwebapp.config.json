{
  "routes": [
    {
      "route": "/new/service-worker.js"
    },
    {
      "route": "/new/service-worker-assets.js"
    },
    {
      "route": "/new/_framework/*"
    },
    {
      "route": "/new/css/*"
    },
    {
      "route": "/new",
      "rewrite": "/new/index.html"
    },
    {
      "route": "/new/*",
      "rewrite": "/new/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/assets/*",
      "/new/*",
      "/*.{css,scss,js,png,jpg,gif,ico,svg,woff2}"
    ]
  }
}
